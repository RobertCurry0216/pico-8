version 32
__lua__
#include waterlevel.p8

function _init()
	waterlevel_init()
end

function _update()

end

function _draw()
	local d = flr(sin(time()/4)*64)+64
	waterlevel(d)
	for x=0,15 do
		rectfill(
			x*8,0,
			x*8+8,127,
			x)
	end
	
	line(0,d-1,127,d-1,7)
end
